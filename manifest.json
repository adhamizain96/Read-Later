{
  "manifest_version": 3,
  "name": "Read Later",
  "description": "This extension allows users to save pages to read later.",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "notifications"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
